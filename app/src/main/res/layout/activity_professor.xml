<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cat.memoriacastello.www.memoriahistorica.Professor"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <EditText
        android:id="@+id/p6et1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:ems="10"
        android:hint="contrasenya"
        android:inputType="textPassword"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="26dp" />

    <EditText
        android:id="@+id/p6et2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="número pregunta"
        android:inputType="number"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/p6et1"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/p6et1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/p6et1" />

    <Button
        android:id="@+id/p6b2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginTop="71dp"
        android:onClick="fesVisible"
        android:text="Vés-hi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/p6tv1"
        android:layout_width="318dp"
        android:layout_height="162dp"
        android:layout_marginTop="81dp"
        android:onClick="fesVisible"
        android:text="enunciat"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/p6et2"
        app:layout_constraintRight_toLeftOf="@+id/p6tv2"
        app:layout_constraintTop_toBottomOf="@+id/p6et2"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteY="193dp"
        tools:layout_editor_absoluteX="42dp" />

    <TextView
        android:id="@+id/p6tv2"
        android:layout_width="326dp"
        android:layout_height="35dp"
        android:onClick="fesVisible"
        android:text="resposta"
        android:visibility="invisible"
        android:layout_marginStart="27dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="93dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="27dp"
        tools:layout_editor_absoluteY="383dp" />

    <Button
        android:id="@+id/p6b1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="arrere"
        android:text="Arrere"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="18dp"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>
